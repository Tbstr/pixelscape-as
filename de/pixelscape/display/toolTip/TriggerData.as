package de.pixelscape.display.toolTip {	import flash.display.DisplayObject;			/**	 * @author Tobias Friese	 */	public class TriggerData 	{		/* variables */		private var _target:DisplayObject;		private var _content:*;				private var _xOff:int;		private var _yOff:int;				private var _delay:Number;				private var _toolTip:ToolTip;				public function TriggerData(target:DisplayObject, content:*, xOff:Number, yOff:Number, delay:Number = 0)		{			this._target = target;			this._content = content;						this._xOff = xOff;			this._yOff = yOff;						this._delay = delay;		}				/* getter setter */		public function get target():DisplayObject		{			return this._target;		}				public function get content():*		{			return this._content;		}				public function get xOff():Number		{			return this._xOff;		}				public function get yOff():Number		{			return this._yOff;		}				public function get delay():Number		{			return this._delay;		}				public function get toolTip():ToolTip		{			return this._toolTip;		}				public function set toolTip(value:ToolTip):void		{			this._toolTip = value;		}	}}